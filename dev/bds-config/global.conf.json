{
  "farm": {
    "name": "DEVFARM",
    "privateKey": "<MySecretFarmPassword>"
  },
  "plugins": {
    "activated": [
      "CRM",
      "CS",      
      "SOC",
      "TMM",
      "PIM",
      "CMS",
      "LIC",
      "SAMPLE"
    ],
    "configurations": {     
      "CMS": {
        "EnableStaticGeneration": true        
      }
    }
  },
  "security": {
    "allowUnSecuredOperation": true    
  },
  "server": {
    "https": false,
    "name": "BDS.DEV.1",
    "corsOrigins": [
      "*"
    ],
    "externalIp": "172.17.0.1"
  },
  "searchIndexing": {
    "enable": true,
    "es": {
      "uri": "http://172.17.0.1:9200"
    }
  },
  "jobs": {
    "enable": true
  },
  "dataStore":{
    "cb": {
      "userName": "Administrator",
      "password": "password",
      "useSsl": false,
      "configPollEnabled": false,
      "enableTcpKeepAlives": false,
      "servers": [
        "http://172.17.0.1:8091/pools"
      ],
      "connectionPool": {
        "name": "custom",
        "maxSize": 5,
        "minSize": 5
      }
    }
  },
  "apm": {
    "serverUrls": "http://172.17.0.1:8200"
  },
  "log": {
    "serverUrls": "http://172.17.0.1:9200"
  }
}